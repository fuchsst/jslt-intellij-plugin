// some comment
import "filename.jslt" as import_alias

let idparts = split(.id, "-")
let xxx = [for ($idparts) "x" * size(.)]

def myFunc(param1, param2)
    let yyy = 10
    if ($param1 > $param2)
        $param1 + $yyy
    else if ($param1 < $param2)
        $param1 - $yyy
    else
        $param2 - $undefined-var

{
    "id" : join($xxx, "-"),
    "factor" : 10.0e+6,
    "calc" : myFunc(random(), 1.23),
    * : .
}