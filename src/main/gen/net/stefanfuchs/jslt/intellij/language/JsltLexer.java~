   * matched text.
   *
   * It is equivalent to yytext().charAt(pos), but faster
   *
   * @param pos the position of the character to fetch.
   *            A value from 0 to yylength()-1.
   *
   * @return the character at position pos
   */
  public final char yycharat(int pos) {
    return zzBuffer.charAt(zzStartRead+pos);
  }


  /**
   * Returns the length of the matched text region.
   */
  public final int yylength() {
    return zzMarkedPos-zzStartRead;
  }


  /**
   * Reports an error that occurred while scanning.
   *
   * In a wellformed scanner (no or only correct usage of
   * yypushback(int) and a match-all fallback rule) this method
   * will only be called with things that "Can't Possibly Happen".
   * If this method is called, something is seriously wrong
   * (e.g. a JFlex bug producing a faulty scanner etc.).
   *
   * Usual syntax/scanner level error handling should be done
   * in error fallback rules.
   *
   * @param   errorCode  the code of the errormessage to display
   */
  private void zzScanError(int errorCode) {
    String message;
    try {
      message = ZZ_ERROR_MSG[errorCode];
    }
    catch (ArrayIndexOutOfBoundsException e) {
      message = ZZ_ERROR_MSG[ZZ_UNKNOWN_ERROR];
    }

    throw new Error(message);
  }


  /**
   * Pushes the specified amount of characters back into the input stream.
   *
   * They will be read again by then next call of the scanning method
   *
   * @param number  the number of characters to be read again.
   *                This number must not be greater than yylength()!
   */
  public void yypushback(int number)  {
    if ( number > yylength() )
      zzScanError(ZZ_PUSHBACK_2BIG);

    zzMarkedPos -= number;
  }


  /**
   * Contains user EOF-code, which will be executed exactly once,
   * when the end of file is reached
   */
  private void zzDoEOF() {
    if (!zzEOFDone) {
      zzEOFDone = true;
    
    }
  }


  /**
   * Resumes scanning until the next regular expression is matched,
   * the end of input is encountered or an I/O-Error occurs.
   *
   * @return      the next token
   * @exception   java.io.IOException  if any I/O-Error occurs
   */
  public IElementType advance() throws java.io.IOException {
    int zzInput;
    int zzAction;

    // cached fields:
    int zzCurrentPosL;
    int zzMarkedPosL;
    int zzEndReadL = zzEndRead;
    CharSequence zzBufferL = zzBuffer;

    int [] zzTransL = ZZ_TRANS;
    int [] zzRowMapL = ZZ_ROWMAP;
    int [] zzAttrL = ZZ_ATTRIBUTE;

    while (true) {
      zzMarkedPosL = zzMarkedPos;

      zzAction = -1;

      zzCurrentPosL = zzCurrentPos = zzStartRead = zzMarkedPosL;

      zzState = ZZ_LEXSTATE[zzLexicalState];

      // set up zzAction for empty match case:
      int zzAttributes = zzAttrL[zzState];
      if ( (zzAttributes & 1) == 1 ) {
        zzAction = zzState;
      }


      zzForAction: {
        while (true) {

          if (zzCurrentPosL < zzEndReadL) {
            zzInput = Character.codePointAt(zzBufferL, zzCurrentPosL/*, zzEndReadL*/);
            zzCurrentPosL += Character.charCount(zzInput);
          }
          else if (zzAtEOF) {
            zzInput = YYEOF;
            break zzForAction;
          }
          else {
            // store back cached positions
            zzCurrentPos  = zzCurrentPosL;
            zzMarkedPos   = zzMarkedPosL;
            boolean eof = zzRefill();
            // get translated positions and possibly new buffer
            zzCurrentPosL  = zzCurrentPos;
            zzMarkedPosL   = zzMarkedPos;
            zzBufferL      = zzBuffer;
            zzEndReadL     = zzEndRead;
            if (eof) {
              zzInput = YYEOF;
              break zzForAction;
            }
            else {
              zzInput = Character.codePointAt(zzBufferL, zzCurrentPosL/*, zzEndReadL*/);
              zzCurrentPosL += Character.charCount(zzInput);
            }
          }
          int zzNext = zzTransL[ zzRowMapL[zzState] + ZZ_CMAP(zzInput) ];
          if (zzNext == -1) break zzForAction;
          zzState = zzNext;

          zzAttributes = zzAttrL[zzState];
          if ( (zzAttributes & 1) == 1 ) {
            zzAction = zzState;
            zzMarkedPosL = zzCurrentPosL;
            if ( (zzAttributes & 8) == 8 ) break zzForAction;
          }

        }
      }

      // store back cached position
      zzMarkedPos = zzMarkedPosL;

      if (zzInput == YYEOF && zzStartRead == zzCurrentPos) {
        zzAtEOF = true;
        zzDoEOF();
        return null;
      }
      else {
        switch (zzAction < 0 ? zzAction : ZZ_ACTION[zzAction]) {
          case 1: 
            { return TokenType.BAD_CHARACTER;
            } 
            // fall through
          case 51: break;
          case 2: 
            { return TokenType.WHITE_SPACE;
            } 
            // fall through
          case 52: break;
          case 3: 
            { return JsltTypes.PLUS;
            } 
            // fall through
          case 53: break;
          case 4: 
            { return JsltTypes.IDENT;
            } 
            // fall through
          case 54: break;
          case 5: 
            { return JsltTypes.INTEGER;
            } 
            // fall through
          case 55: break;
          case 6: 
            { return JsltTypes.MINUS;
            } 
            // fall through
          case 56: break;
          case 7: 
            { return JsltTypes.DOT;
            } 
            // fall through
          case 57: break;
          case 8: 
            { return JsltTypes.SLASH;
            } 
            // fall through
          case 58: break;
          case 9: 
            { return JsltTypes.COLON;
            } 
            // fall through
          case 59: break;
          case 10: 
            { return JsltTypes.ASSIGN;
            } 
            // fall through
          case 60: break;
          case 11: 
            { return JsltTypes.LPAREN;
            } 
            // fall through
          case 61: break;
          case 12: 
            { return JsltTypes.COMMA;
            } 
            // fall through
          case 62: break;
          case 13: 
            { return JsltTypes.RPAREN;
            } 
            // fall through
          case 63: break;
          case 14: 
            { return JsltTypes.LBRACKET;
            } 
            // fall through
          case 64: break;
          case 15: 
            { return JsltTypes.RBRACKET;
            } 
            // fall through
          case 65: break;
          case 16: 
            { return JsltTypes.LCURLY;
            } 
            // fall through
          case 66: break;
          case 17: 
            { return JsltTypes.RCURLY;
            } 
            // fall through
          case 67: break;
          case 18: 
            { return JsltTypes.BIGGER;
            } 
            // fall through
          case 68: break;
          case 19: 
            { return JsltTypes.SMALLER;
            } 
            // fall through
          case 69: break;
          case 20: 
            { return JsltTypes.STAR;
            } 
            // fall through
          case 70: break;
          case 21: 
            { return JsltTypes.PIPE;
            } 
            // fall through
          case 71: break;
          case 22: 
            { yybegin(YYINITIAL); return JsltTypes.IMPORT_ALIAS;
            } 
            // fall through
          case 72: break;
          case 23: 
            { return JsltTypes.VARIABLE_DECL;
            } 
            // fall through
          case 73: break;
          case 24: 
            { yybegin(YYINITIAL); return JsltTypes.ASSIGN;
            } 
            // fall through
          case 74: break;
          case 25: 
            { return JsltTypes.FUNCTION_DECL_NAME;
            } 
            // fall through
          case 75: break;
          case 26: 
            { yybegin(FUNC_DECLARATION_PARAMS); return JsltTypes.LPAREN;
            } 
            // fall through
          case 76: break;
          case 27: 
            { return JsltTypes.FUNCTION_DECL_PARAM;
            } 
            // fall through
          case 77: break;
          case 28: 
            { yybegin(YYINITIAL); return JsltTypes.RPAREN;
            } 
            // fall through
          case 78: break;
          case 29: 
            { return JsltTypes.COMMENT;
            } 
            // fall through
          case 79: break;
          case 30: 
            { return JsltTypes.STRING;
            } 
            // fall through
          case 80: break;
          case 31: 
            { return JsltTypes.VARIABLE;
            } 
            // fall through
          case 81: break;
          case 32: 
            { return JsltTypes.IF;
            } 
            // fall through
          case 82: break;
          case 33: 
            { return JsltTypes.OR;
            } 
            // fall through
          case 83: break;
          case 34: 
            { return JsltTypes.EQUALS;
            } 
            // fall through
          case 84: break;
          case 35: 
            { return JsltTypes.UNEQUALS;
            } 
            // fall through
          case 85: break;
          case 36: 
            { return JsltTypes.BIGOREQ;
            } 
            // fall through
          case 86: break;
          case 37: 
            { return JsltTypes.SMALLOREQ;
            } 
            // fall through
          case 87: break;
          case 38: 
            { return JsltTypes.IMPORT_FILE_STRING;
            } 
            // fall through
          case 88: break;
          case 39: 
            { return JsltTypes.AS;
            } 
            // fall through
          case 89: break;
          case 40: 
            { return JsltTypes.PIDENT;
            } 
            // fall through
          case 90: break;
          case 41: 
            { return JsltTypes.DECIMAL;
            } 
            // fall through
          case 91: break;
          case 42: 
            { yybegin(LETS); return JsltTypes.LET;
            } 
            // fall through
          case 92: break;
          case 43: 
            { return JsltTypes.FOR;
            } 
            // fall through
          case 93: break;
          case 44: 
            { return JsltTypes.AND;
            } 
            // fall through
          case 94: break;
          case 45: 
            { yybegin(FUNC_DECLARATION); return JsltTypes.DEF;
            } 
            // fall through
          case 95: break;
          case 46: 
            { return JsltTypes.NULL;
            } 
            // fall through
          case 96: break;
          case 47: 
            { return JsltTypes.ELSE;
            } 
            // fall through
          case 97: break;
          case 48: 
            { return JsltTypes.TRUE;
            } 
            // fall through
          case 98: break;
          case 49: 
            { return JsltTypes.FALSE;
            } 
            // fall through
          case 99: break;
          case 50: 
            { yybegin(IMPORTS); return JsltTypes.IMPORT;
            } 
            // fall through
          case 100: break;
          default:
            zzScanError(ZZ_NO_MATCH);
          }
      }
    }
  }


}
